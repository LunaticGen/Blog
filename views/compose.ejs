<%- include("partials/header") -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
    <div class="form-group">
        <label>Title</label>
        <input type="text" name="postTitle" class="form-control" placeholder="Title of Your Post">
    </div>
    <div class="form-group">
        <label>Post</label>
        <textarea  name = "postContent" class="form-control" placeholder="Start Your Post"></textarea>
    </div>
    <button type="submit" name="submit" class="btn btn-primary">Publish</button>
</form>

<%- include('partials/footer'); %>